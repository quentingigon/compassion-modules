<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="survey_user_input_view_view_tree" model="ir.ui.view">
            <field name="model">survey.user_input</field>
            <field name="inherit_id" ref="survey.survey_user_input_form"/>
            <field name="name">Phone survey</field>
            <field name="arch" type="xml">
                <field name="email" position="after">
                    <field name="phone" string="Call phone"/>
                    <field name="mobile" string="Call mobile"/>
                </field>
                <field name="token" position="after">
                    <field name="survey_link" string="Link to survey" widget="url"/>
                </field>
            </field>
        </record>
    </data>
</odoo>
